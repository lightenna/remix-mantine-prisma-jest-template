{
    "name": "gahmuret",
    "version": "0.2.1",
    "private": true,
    "sideEffects": false,
    "scripts": {
        "dev": "npx dotenv -e .env.local -- cross-env NODE_OPTIONS=\"--require newrelic\" DEBUG=gahmuret:* remix vite:dev",
        "test-jest": "npx dotenv -e .env.local -- jest --config jest.config.mjs",
        "bump-version": "npm version patch",
        "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint . && tsc",
        "build": "remix vite:build",
        "migrate dev app-staging": "npx dotenv -e .env.local -- prisma migrate dev",
        "migrate deploy app-prod": "npx dotenv -e .env.prod -- prisma migrate deploy",
        "build-container": "docker build -t gahmuret-docker -f ./Dockerfile ./",
        "run-container": "docker run -p 4000:3000 --env-file ./.env.local gahmuret-docker",
        "docker-start": "npm run postinstall && npm run start",
        "postinstall": "prisma generate",
        "start": "cross-env NODE_OPTIONS=\"--require newrelic\" remix-serve ./build/server/index.js"
    },
    "type": "module",
    "packageManager": "pnpm@9.11.0",
    "dependencies": {
        "@mantine/core": "^7.15.2",
        "@mantine/hooks": "^7.15.2",
        "@mantine/modals": "^7.15.2",
        "@mantine/notifications": "^7.15.2",
        "@prisma/client": "^5.22.0",
        "@remix-run/node": "^2.15.2",
        "@remix-run/react": "^2.15.2",
        "@remix-run/serve": "^2.15.2",
        "@remix-run/server-runtime": "^2.15.2",
        "@remix-run/testing": "^2.15.2",
        "@types/newrelic": "^9.14.7",
        "accept-language-parser": "^1.5.0",
        "debug": "^4.4.0",
        "dotenv": "^16.4.5",
        "isbot": "^5.1.9",
        "newrelic": "^12.10.0",
        "prisma": "^5.22.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "remix-auth": "^4.1.0",
        "remix-auth-oauth2": "^3.2.2",
        "use-intl": "^3.15.3",
        "winston": "^3.17.0",
        "winston-newrelic-agent-transport": "^1.0.8"
    },
    "pnpm": {
        "overrides": {
            "newrelic": "$newrelic"
        }
    },
    "devDependencies": {
        "@quramy/jest-prisma": "^1.8.1",
        "@remix-run/dev": "^2.15.2",
        "@remix-run/eslint-config": "^2.15.2",
        "@swc/core": "^1.6.6",
        "@swc/jest": "^0.2.36",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^14.3.1",
        "@testing-library/react-hooks": "^8.0.1",
        "@types/eslint": "^8.56.10",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.14.2",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "cross-env": "^7.0.3",
        "dotenv-cli": "^7.4.2",
        "eslint": "^8.38.0",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-fetch-mock": "^3.0.3",
        "postcss": "^8.4.38",
        "postcss-preset-mantine": "^1.17.0",
        "postcss-selector-parser": "^6.0.16",
        "postcss-simple-vars": "^7.0.1",
        "prettier": "^3.4.2",
        "sass": "^1.77.6",
        "typescript": "^5.5.2",
        "vite": "^5.3.1",
        "vite-tsconfig-paths": "^4.3.2",
        "logform": "^2.7.0"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "license": "Apache-2.0",
    "author": "Alex Stanhope for lightenna.com"
}
